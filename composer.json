{
  "name": "colesnic89/yii2-imgopt",
  "version": "1.3.9",
  "description": "Image optimization widget for Yii2 Framework with auto WebP/AVIF image format generation from PNG/JPG files.",
  "type": "yii2-extension",
  "keywords": [
    "image",
    "optimization",
    "yii2",
    "webp",
    "avif",
    "jpg",
    "png",
    "generator"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Bartosz WÃ³jcik",
      "email": "support@pelock.com",
      "homepage": "https://www.pelock.com"
    },
    {
      "name": "Maxim Colesnic",
      "email": "colesnic89@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": ">=2.0.38",
    "ext-gd": "*",
    "ext-exif": "*",
    "gumlet/php-image-resize": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "PELock\\ImgOpt\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
